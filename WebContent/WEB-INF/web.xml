<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<servlet>  
	    <servlet-name>facesServlet</servlet-name>   
	    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>   
	    <load-on-startup>1</load-on-startup>   
	</servlet>
	<servlet-mapping>
	    <servlet-name>facesServlet</servlet-name>   
	    <url-pattern>*.xhtml</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	    <welcome-file>index.xhtml</welcome-file>
	</welcome-file-list>
	<filter>
	    <filter-name>CredFilter</filter-name>
	    <filter-class>me.langker.LendingPlat.Util.URLFilter</filter-class>
	</filter>
	<filter-mapping>
	    <filter-name>CredFilter</filter-name>
	    <url-pattern>/upload_cred/*</url-pattern>
	</filter-mapping>
	<filter>
	    <filter-name>VersionFilter</filter-name>
	    <filter-class>me.langker.LendingPlat.Util.VersionFilter</filter-class>
	</filter>
	<filter-mapping>
	    <filter-name>VersionFilter</filter-name>
	    <url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>